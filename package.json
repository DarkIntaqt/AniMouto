{
  "name": "animouto",
  "version": "3.0.0",
  "displayName": "AniMouto",
  "author": "Nicholas \"TehNut\" Ignoffo",
  "description": "Let an imouto enhance your AniList experience.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:chromium": "vite build --mode chromium",
    "build:firefox": "vite build --mode firefox",
    "watch:chromium": "vite build --watch --mode chromium",
    "watch:firefox": "vite build --watch --mode firefox",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/firefox",
    "serve:chromium": "web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/chromium",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "introspect": "node ./scripts/createIntrospection.js"
  },
  "dependencies": {
    "@floating-ui/dom": "0.4.2",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@tailwindcss/line-clamp": "0.3.1",
    "@urql/exchange-graphcache": "4.3.6",
    "@urql/svelte": "1.3.3",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "jwt-decode": "3.1.2",
    "svelte-fa": "2.4.0",
    "svelte-lazy": "1.1.0",
    "svelte-spa-router": "3.2.0",
    "timeago.js": "4.0.2",
    "webext-storage-cache": "5.0.0",
    "webextension-polyfill": "0.9.0"
  },
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "0.1.8",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.40",
    "@tsconfig/svelte": "3.0.0",
    "@types/webextension-polyfill": "0.8.3",
    "@urql/introspection": "0.3.2",
    "autoprefixer": "10.4.4",
    "node-fetch": "^3.2.3",
    "node-sass": "7.0.1",
    "postcss": "8.4.12",
    "svelte": "3.46.6",
    "svelte-check": "2.4.6",
    "svelte-preprocess": "4.10.4",
    "tailwindcss": "3.0.23",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "vite": "2.9.1",
    "web-ext": "6.8.0"
  }
}
