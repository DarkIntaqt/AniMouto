{
  "name": "AniMouto",
  "short_name": "AniMouto",
  "author": "TehNut",
  "version": "1.0.13",
  "description": "A little sister to enhance your AniList experience.",
  "key": "",
  "offline_enabled": false,
  "permissions": [
    "storage",
    "alarms",
    "identity",
    "*://*.anilist.co/*"
  ],
  "optional_permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "bg/oauth_handler.js",
      "bg/alarms.js",
      "bg/anilist_util.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/animouto_16x.png",
      "32": "img/animouto_32x.png",
      "48": "img/animouto_48x.png",
      "128": "img/animouto_128x.png"
    }
  },
  "icons": {
    "16": "img/animouto_16x.png",
    "32": "img/animouto_32x.png",
    "48": "img/animouto_48x.png",
    "128": "img/animouto_128x.png"
  },
  "web_accessible_resources": [
    "/oauth2/*"
  ],
  "manifest_version": 2
}
